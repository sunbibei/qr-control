<?xml version="1.0" encoding="utf-8"?>
<control name="qr-control-configure" >

  <gait name="gait-repository">
    <creep auto_inst="qr_control::Creep" name="creep" >
      <count description="initilize the __PrivateParam"
        init_pose="300" cali_imu="300" stance="1000" height="300" swing="1000" />
    </creep>
  </gait>

  <robot >
    <lf auto_inst="qr_control::RobotLeg" jnt="fl" origin="fl_pos fl_cmd_pos fl_foot_force" />
    <lf auto_inst="qr_control::RobotLeg" jnt="fr" origin="fr_pos fr_cmd_pos fr_foot_force" />
    <lf auto_inst="qr_control::RobotLeg" jnt="hl" origin="hl_pos hl_cmd_pos hl_foot_force" />
    <lf auto_inst="qr_control::RobotLeg" jnt="hr" origin="hr_pos hr_cmd_pos hr_foot_force" />
  </robot>

</control>